{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "acls": []
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "product": {
    "dataSource": "db",
    "public": true
  },
  "Usuario": {
    "dataSource": "db",
    "public": true
  },
  "Upload": {
    "dataSource": "localFiles",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true,
          "download": true,
          "newBase64File": true,
          "getFilesToZip": true,
          "upload":true
        }
      }
    }
  },
  "UploadedFiles": {
    "dataSource": "db",
    "public": true
  },
  "Meta":{
    "dataSource":"db",
    "public":true
  },
  "Client":{
    "dataSource":"db",
    "public":true
  }
}

{
  "_meta": {
    "sources": [
      "loopback/common/models",
      "loopback/server/models",
      "../common/models",
      "./models"
    ],
    "mixins": [
      "loopback/common/mixins",
      "loopback/server/mixins",
      "../common/mixins",
      "./mixins"
    ]
  },
  "User": {
    "dataSource": "db",
    "acls": []
  },
  "AccessToken": {
    "dataSource": "db",
    "public": false
  },
  "ACL": {
    "dataSource": "db",
    "public": false
  },
  "RoleMapping": {
    "dataSource": "db",
    "public": false,
    "options": {
      "strictObjectIDCoercion": true
    }
  },
  "Role": {
    "dataSource": "db",
    "public": false
  },
  "product": {
    "dataSource": "db",
    "public": true
  },
  "Usuario": {
    "dataSource": "db",
    "public": true
  },
  "Upload": {
    "dataSource": "localFiles",
    "public": true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": true,
          "download": true,
          "newBase64File": true,
          "getFilesToZip": true,
          "upload":true
        }
      }
    }
  },
  "UploadedFiles": {
    "dataSource": "db",
    "public": true
  },
  "Meta":{
    "dataSource":"db",
    "public":true
  },
  "Client":{
    "dataSource":"db",
    "public":true
  },
  "Nota": {
    "dataSource": "db",
    "public":true
  },
  "Catalogo": {
    "dataSource": "db",
    "public":true
  },
  "Trato": {
    "dataSource": "db",
    "public":true
  },
  "CategoriaSub": {
    "dataSource": "db",
    "public":true
  },
  "Subtarea": {
    "dataSource": "db",
    "public":true
  },
  "Archivo": {
    "dataSource": "db",
    "public":true
  },
  "ProductoFijo": {
    "dataSource": "db",
    "public":true
  },
  "Tablon": {
    "dataSource": "db",
    "public":true
  },
  "Cotizacion": {
    "dataSource": "db",
    "public":true
  },
  "ProductoCreado": {
    "dataSource": "db",
    "public":true
  },
  "Informe": {
    "dataSource": "db",
    "public":true
  },
  "Email": {
    "dataSource": "Email",
    "public":true
  },
  "Mail":{
    "dataSource":"Email",
    "public":true,
    "options": {
      "remoting": {
        "sharedMethods": {
          "*": false,
          "sendEmail":true
        }
      }
    }
  }
}

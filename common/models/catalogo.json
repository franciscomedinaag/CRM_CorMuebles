{
    "name": "Catalogo",
    "plural": "Catalogos",
    "idInjection": true,
    "options": {
      "validateUpsert": true
    },
    "properties": {
      "nombre": {
        "type": "string",
        "required": "true"
      },
      "file": {
        "type": "string",
        "required": "true"
      }
    },
    "validations": [],
    "relations": {
     
    },
    "acls": [
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "$unauthenticated",
        "permission": "DENY"
      },
      {
        "accessType": "*",
        "principalType": "ROLE",
        "principalId": "$authenticated",
        "permission": "ALLOW"
      }
    ],
    "methods": {
      "setFile": {
        "accepts": [
          {
            "arg": "newFile",
            "type": "object",
            "required": true,
            "description": "file to save",
            "http": {
              "source": "body"
            }
          }
        ],
        "returns": [
          {
            "arg": "file",
            "type": "object",
            "root": true,
            "description": "new file"
          }
        ],
        "description": " Add catalog ",
        "http": [
          {
            "path": "/setFile",
            "verb": "post"
          }
        ]
      },
      "deleteFile": {
        "accepts": [
          {
            "arg": "oldFile",
            "type": "object",
            "required": true,
            "description": "file to delete",
            "http": {
              "source": "body"
            }
          }
        ],
        "returns": [
          {
          }
        ],
        "description": " Delete File ",
        "http": [
          {
            "path": "/deleteFile",
            "verb": "post"
          }
        ]
      }
    }
  }
  